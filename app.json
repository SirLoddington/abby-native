{
  "expo": {
    "jsEngine": "jsc",
    "name": "abby-native",
    "slug": "abby-native",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/transparent100x100.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/transparent200x200.gif",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
